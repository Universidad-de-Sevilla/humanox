{
  "name": "US/Humanox",
  "description": "Portal and Intranet based on silex and doctrine at University of Seville",
  "license": "MIT",
  "type": "project",
  "require-dev": {
    "phpunit/phpunit": "3.7.*",
    "symfony/browser-kit": "^3.0",
    "symfony/css-selector": "^3.0"
  },
  "require": {
    "dflydev/doctrine-orm-service-provider": "2.0.*@dev",
    "jasig/phpcas": "1.3.*",
    "monolog/monolog": "~1.17",
    "php": ">=5.5.9",
    "silex/silex": "~2.0",
    "silex/web-profiler": "~2.0",
    "symfony/class-loader": "~2.8",
    "symfony/config": "~2.8",
    "symfony/console": "~2.8",
    "symfony/debug": "~2.8",
    "symfony/doctrine-bridge": "~2.8",
    "symfony/finder": "~2.8",
    "symfony/form": "~2.8",
    "symfony/monolog-bridge": "~2.8",
    "symfony/process": "~2.8",
    "symfony/security": "~2.8",
    "symfony/twig-bridge": "~2.8",
    "symfony/validator": "~2.8",
    "symfony/var-dumper": "~2.8",
    "symfony/yaml": "~2.8"
  },
  "autoload": {
    "psr-4": {
      "US\\Humanox\\": "src/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  },
  "scripts": {
    "run": [
      "echo 'Started web server on http://localhost:8888'",
      "php -S localhost:8888 -t web web/index_dev.php"
    ]
  }
}
